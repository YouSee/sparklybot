{
  "name": "sparklybot",
  "version": "1.0.2",
  "description": "Automation framework for the Spark UI Browser",
  "repository": "YouSee/sparklybot",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files --extensions '.ts' --extensions '.js'",
    "clean": "del-cli dist",
    "lint": "eslint --cache src",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "prepare": "npm run build",
    "release": "standard-version",
    "security": "npm audit",
    "test": "jest './test/local/.*.spark.test.js' --runInBand",
    "test:remote": "jest './test/remote/.*.spark.test.js' --runInBand",
    "test:circleCI": "jest './test/local/.*.spark.test.js' --runInBand",
    "pretest": "npm run lint"
  },
  "keywords": [
    "rdk",
    "spark",
    "ui",
    "testing",
    "automation",
    "ci"
  ],
  "author": "Yousee",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.0",
    "@types/express-http-proxy": "^1.5.11",
    "@types/node": "^12.6.6",
    "@types/request-promise": "^4.1.44",
    "@types/uid2": "0.0.0",
    "@types/ws": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "cross-env": "^5.2.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "tree-kill": "^1.2.1",
    "uid2": "0.0.3",
    "ws": "^7.1.0"
  }
}
